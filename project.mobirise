          "_styles": {
            "padding-top": "(@paddingTop * 1rem)",
            "padding-bottom": "(@paddingBottom * 1rem)",
            "& when (@bg-type = \"color\")": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = \"image\")": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".title-wrap, .title": {
              "width": "100%",
              "padding": "0 1rem"
            },
            ".list-item": {
              "padding": "1rem 0"
            },
            ".mbr-section-btn": {
              "margin": "0"
            },
            ".card-head": {
              "padding-top": "0",
              "padding-bottom": "0rem"
            },
            ".card": {
              "&:hover": {
                ".btn": {
                  "background-color": "@primaryColor",
                  "color": "#ffffff !important",
                  "&:hover": {
                    "background-color": "black !important"
                  }
                }
              }
            },
            ".card-inner": {
              "padding": "2.5rem 3rem"
            },
            "*": {
              "word-break": "break-word"
            },
            ".items-list-wrap .list-item": {
              "& when (@strippedBg) and not (@transparentBg)": {
                "&:nth-child(odd)": {
                  "background-color": "@strippedBgColor"
                }
              },
              "& when (@showIcons)": {
                "display": "flex",
                "align-items": "center"
              }
            },
            "& when (@showIcons)": {
              ".item-ico": {
                "min-width": "35px",
                "color": "@primaryColor",
                "font-size": "~\"@{iconsSize}rem\"",
                "margin-right": "1rem"
              }
            },
            "& when not (@transparentBg)": {
              "& when not (@checkDifferentBg)": {
                ".card-inner, .card-head, .card-price-wrap, .list-item": {
                  "background-color": "@cardBg"
                }
              },
              "& when (@checkDifferentBg)": {
                ".card1": {
                  ".card-inner, .card-head, .card-price-wrap, .list-item": {
                    "background-color": "@card1Bg"
                  }
                },
                ".card2": {
                  ".card-inner, .card-head, .card-price-wrap, .list-item": {
                    "background-color": "@card2Bg"
                  }
                },
                ".card3": {
                  ".card-inner, .card-head, .card-price-wrap, .list-item": {
                    "background-color": "@card3Bg"
                  }
                },
                ".card4": {
                  ".card-inner, .card-head, .card-price-wrap, .list-item": {
                    "background-color": "@card4Bg"
                  }
                }
              }
            },
            ".btn": {
              "box-shadow": "2.5px 4.33px 30px 0 rgba(64, 87, 191, 0.2)",
              "border": "0px",
              "margin": "0",
              "width": "-webkit-fill-available",
              "&:hover": {
                "border": "0px",
                "box-shadow": "none"
              }
            },
            "& when (@transparentBg)": {
              ".card-inner, .card-price-wrap, .list-item": {
                "background": "transparent"
              },
              ".card-inner": {
                "position": "relative",
                ".mbr-overlay": {
                  "opacity": "@bgOpacity",
                  "background-color": "@cardBg"
                },
                "div:not(.mbr-overlay)": {
                  "z-index": "1"
                }
              }
            },
            "& when (@cardBorder)": {
              ".card-head, .card-price-wrap, .list-item": {
                "border-bottom": "2px solid @colorBorder"
              }
            },
            "@media (max-width: 767px)": {
